{
  "name": "@pika/plugin-import-yapi",
  "version": "1.0.0",
  "author": "kamalyes",
  "description": "Import APIs from YApi to Pika.",
  "title": "Import from YApi",
  "main": "dist/index.js",
  "logo": "https://data.eolink.com/2uPpVBif783161525c0c5148451e945b1f014b41ba3aa4b",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.38",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamalyes/pika-extensions.git"
  },
  "categories": [
    "Data Migration"
  ],
  "features": {
    "importAPI": {
      "action": "importFunc",
      "type": "file",
      "label": "YApi(.json)",
      "description": "${function.description}",
      "icon": "https://data.eolink.com/2uPpVBif783161525c0c5148451e945b1f014b41ba3aa4b"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
